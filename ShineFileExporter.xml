<!-- Configuration of Module ShineFileExporter -->

<ShineFileExporter
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation='[SCHEMAPATH]/ShineFileExporter.xsd'>

  <OutputFilename> shineOffline.root </OutputFilename>

  <BranchStatusOff>
      runTree.fRun.fBOSRecord
      eventTree.fEvent.fBOSRecord
      eventTree.fEvent.fRawEvent
      eventTree.fEvent.fProcEvent
  </BranchStatusOff>

  <BranchStatusOn>
    eventTree.fEvent.fRawEvent.fDCS
    eventTree.fEvent.fRawEvent.fBeam.fTrigger
    eventTree.fEvent.fRawEvent.fSilicon
  </BranchStatusOn>


</ShineFileExporter>
